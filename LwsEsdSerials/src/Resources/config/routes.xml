<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        https://symfony.com/schema/routing/routing-1.0.xsd">

    <import resource="../../**/Core/Checkout/*/Api/*Api.php" type="attribute"/>
    <import resource="../../**/Core/Checkout/*/StoreApi/*StoreApi.php" type="attribute"/>
    <import resource="../../**/Core/Content/*/Api/*Api.php" type="annotation"/>
    <import resource="../../**/Core/Content/*/StoreApi/*StoreApi.php" type="attribute"/>

    <import resource="../../**/Storefront/Controller/*Controller.php" type="attribute"/>
</routes>
